<?xml version="1.0" encoding="UTF-8"?>
<workbook>
	<worksheet name="Sheet1">
		<section startRow="0" endRow="0">
		</section>	
		<loop startRow="1" endRow="1" items="datas" var="data" varType="cn.bocon.entity.CardData">
			<section startRow="1" endRow="1">
				<mapping row="1" col="0">data.userId</mapping>
				<mapping row="1" col="1">data.name</mapping>
				<mapping row="1" col="2">data.cardTime</mapping>
			</section>
			<loopbreakcondition>
				<rowcheck offset="0">
					<cellcheck offset="0"></cellcheck>
				</rowcheck>
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook> 